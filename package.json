{
  "name": "react-clickable-hoc",
  "version": "1.1.0",
  "description": "Executes onClick to keyboard actions for improved accessibility of your React components",
  "main": "./dist/clickable.js",
  "author": "Neil Bryson Cargamento <neilbryson@live.com>",
  "keywords": [
    "react",
    "hoc",
    "higher order component",
    "clickable",
    "accessibility"
  ],
  "devDependencies": {
    "@neilbryson/eslint-config": "^1.2.0",
    "@babel/core": "^7.4.5",
    "@babel/preset-env": "^7.4.5",
    "@babel/preset-react": "^7.0.0",
    "babel-loader": "^8.0.6",
    "enzyme": "^3.8.0",
    "enzyme-adapter-react-16": "^1.7.1",
    "eslint": "^6.0.0",
    "husky": "~2.4.1",
    "jest": "^24.8.0",
    "jest-enzyme": "^7.0.1",
    "lint-staged": "^8.2.1",
    "path": "^0.12.7",
    "react": "^16.6.3",
    "react-dom": "^16.6.3",
    "regenerator-runtime": "^0.13.1",
    "webpack": "^4.35.0",
    "webpack-cli": "^3.3.4"
  },
  "peerDependencies": {
    "react": "^16.0.0",
    "react-dom": "^16.0.0"
  },
  "license": "MIT",
  "homepage": "https://github.com/neilbryson/react-clickable-hoc",
  "repository": {
    "type": "git",
    "url": "https://github.com/neilbryson/react-clickable-hoc.git"
  },
  "lint-staged": {
    "src/**/*.js": [
      "prettier-eslint --eslint-config-path \".eslintrc\" --print-width 120 --single-quote --write",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}",
      "!<rootDir>/node_modules/"
    ],
    "coverageReporters": [
      "text",
      "text-summary"
    ],
    "setupFilesAfterEnv": [
      "<rootDir>/src/config/testSetup.js"
    ]
  },
  "scripts": {
    "build": "webpack",
    "test": "jest --watch --verbose=false",
    "test:once": "jest"
  }
}
